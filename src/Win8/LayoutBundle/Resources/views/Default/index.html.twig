    'https://code.jquery.com/jquery-1.11.1.min.js'
    '@Win8LayoutBundle/Resources/public/css/bootstrap/dist/js/bootstrap.min.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>

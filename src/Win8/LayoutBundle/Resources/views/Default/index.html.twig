    {% block stylesheets %}
        {% stylesheets
        '@Win8LayoutBundle/Resources/public/css/bootstrap/dist/css/bootstrap.min.css'
        '@Win8LayoutBundle/Resources/public/css/style.css'
        %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet">
